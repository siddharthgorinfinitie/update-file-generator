{
  "name": "siddharthgor/update-file-generator",
  "description": "Laravel package for generating fresh install and date-based update packages",
  "version": "1.0.1",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "update",
    "package",
    "generator"
  ],
  "homepage": "https://github.com/siddharthgorinfinitie/update-file-generator",
  "autoload": {
    "psr-4": {
      "Siddharthgor\\UpdateFileGenerator\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Siddharth Gor",
      "email": "infinitietechnologies09@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "symfony/process": "^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Siddharthgor\\UpdateFileGenerator\\UpdateFileGeneratorServiceProvider"
      ]
    }
  }
}

